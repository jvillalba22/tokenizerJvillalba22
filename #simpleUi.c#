#include <stdio.h>

int main(){
 
  char c[100]; //Strings do not exist on C so create a arrar of char

  printf(">");
  fgets(c, sizeof(c), stdin);
  while(c[1] != '\0'){
      printf("%s", c);
      printf(">");
      fgets(c, sizeof(c), stdin);
    }
}
